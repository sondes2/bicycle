{% extends 'back.html.twig' %}

{% block main %}
    <h1>Livreur</h1>

    <table>
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ livreur.id }}</td>
        </tr>
        <tr>
            <th>Livnom</th>
            <td>{{ livreur.livNom }}</td>
        </tr>
        <tr>
            <th>Livprenom</th>
            <td>{{ livreur.livPrenom }}</td>
        </tr>
        <tr>
            <th>Livemail</th>
            <td>{{ livreur.livEmail }}</td>
        </tr>
        <tr>
            <th>Livlogin</th>
            <td>{{ livreur.livLogin }}</td>
        </tr>
        <tr>
            <th>Livpassword</th>
            <td>{{ livreur.livPassword }}</td>
        </tr>
        <tr>
            <th>Livtel</th>
            <td>{{ livreur.livTel }}</td>
        </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('livreur_affich') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('livreur_update', { 'id': livreur.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
            <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
