<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EventBundle\Entity\Workshop" table="workshop">
    <indexes>
      <index name="idTrainer" columns="idTrainer"/>
    </indexes>
    <id name="idw" type="integer" column="idW">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="namew" type="string" column="NameW" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datew" type="string" column="DateW" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="duration" type="string" column="Duration" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="addressw" type="string" column="AddressW" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idtrainer" type="integer" column="idTrainer" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
