<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EventBundle\Entity\Trainer" table="trainer">
    <id name="idtrainer" type="integer" column="idTrainer">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="namet" type="string" column="NameT" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cint" type="string" column="CinT" length="8" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="speciality" type="string" column="Speciality" length="20" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
