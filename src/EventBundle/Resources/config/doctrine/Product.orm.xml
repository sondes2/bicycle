<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EventBundle\Entity\Product" table="product">
    <id name="idprod" type="integer" column="idProd">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomprod" type="string" column="nomProd" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prixprod" type="integer" column="prixProd" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="quantite" type="integer" column="quantite" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="descrip" type="string" column="descrip" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="type" type="string" column="type" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="categorie" type="string" column="categorie" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
