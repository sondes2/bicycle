<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://www.doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EventBundle\Entity\Competition" table="competition">
    <id name="idcompetition" type="integer" column="idCompetition">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomcomp" type="string" column="nomComp" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="emplacement" type="string" column="emplacement" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datecomp" type="date" column="dateComp" nullable="false"/>
  </entity>
</doctrine-mapping>
